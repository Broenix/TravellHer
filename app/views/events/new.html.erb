<p>new event page</p>

<%= simple_form_for [ @spot, @event]  do |f| %>
        <%= f.input :name, label: 'Nom', class:'label' %>
        <%= f.input :description, label: "Description", class:'label' %>
        <%= f.input :photo, as: :file, label: 'Image', class:'label'%>
          <%= f.input :event_date, html5: true, label: "Event date" %>
        <%= f.submit "Create an event", class: 'btn btn-essential' %>
      <% end %>

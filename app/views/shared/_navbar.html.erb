<div class="wrapper-navbar <%= "bg-white" if params[:action] != 'show' %>" id="sticky">
  <div>
    <%= link_to "/categories",  class: "navbar-brand" do %>
      <img src="<%= asset_path "LOGO.png" %>" alt="">

    <% end %>
  </div>
  <div class="profile-link-img">
    <% if  current_user%>
      <%= image_tag current_user.photo %>
    <% else %>
    <img src="<%= asset_path "logo1.jpg" %>" alt="">
    <% end %>
  </div>
</div>
